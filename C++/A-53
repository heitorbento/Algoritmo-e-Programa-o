#include <cstdlib>
#include <iostream>

using namespace std;

int main(){
    int vetor1[10];
    int vetor2[10];
    int vetor3[10];
    int i;
    
    for(i=0; i<10; i++){
        cout << "Digite o " << i+1 << " valor do VETOR: ";
        cin >> vetor1[i];
    }
    cout << endl;
    for(i=0; i<10; i++){
        cout << "Digite o " << i+1 << " valor do VETOR: ";
        cin >> vetor2[i];
    }
    cout << endl;
    cout << "Os valores digitados no VETOR 1 sao: ";
    for(i=0; i<10; i++){
        cout << " " << vetor1[i] << " -";
    }
    cout << endl;
    cout << "Os valores digitados no VETOR 2 sao: ";
    for(i=0; i<10; i++){
        cout << " " << vetor2[i] << " -";
    }
    for(i=0; i<10; i++){
        vetor3[i]=vetor1[i]*vetor2[i];
    }
    cout << endl;
    cout << "Os valores dos vetores multiplicados sao: ";
    for(i=0; i<10; i++){
        cout << " " << vetor3[i] << " -";
    }
    cout << endl;
    cout << endl;
}

