#include <cstdlib>
#include <iostream>
using namespace std;

int main(int argc, char** argv) {
    char n[30];
    int a=0, s=0, i, idade=0;
    for(i = 1; i <= 5; i++){
        cout << "Informe o primeiro nome: ";
        cin >> n;
        cout << "Informe o ano de nascimento: ";
        cin >> a;
        
        idade = 2024 - a;
        
        s += idade;
    }
    cout << "A soma das idades deu: " << s << endl;
}

